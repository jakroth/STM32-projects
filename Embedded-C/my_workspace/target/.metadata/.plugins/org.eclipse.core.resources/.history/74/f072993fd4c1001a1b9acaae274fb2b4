#ifndef MAIN_H_
#define MAIN_H_

#include <stdint.h>

typedef struct
{
	uint32_t gpioa_en		:1;
	uint32_t gpiob_en		:1;
	uint32_t gpioc_en		:1;
	uint32_t gpiod_en		:1;
	uint32_t gpioe_en		:1;
	uint32_t gpiof_en		:1;
	uint32_t gpiog_en		:1;
	uint32_t gpioh_en		:1;
	uint32_t gpioi_en		:1;
	uint32_t gpioj_en		:1;
	uint32_t gpiok_en		:1;
	uint32_t reserved		:1;
	uint32_t crc_en			:1;
	uint32_t reserved		:5;
	uint32_t bkpsram_en		:1;
	uint32_t reserved		:1;
	uint32_t ccmdataram_en	:1;
	uint32_t dma1_en		:1;
	uint32_t dma2_en		:1;
	uint32_t dma2d_en		:1;
	uint32_t reserved		:1;
	uint32_t ethmac_en		:1;
	uint32_t ethmactx_en	:1;
	uint32_t ethmacrx_en	:1;
	uint32_t otghs_en		:1;
	uint32_t otghsulpi_en	:1;
	uint32_t reserved		:1;
} RCC_AHB1ENR_t;





#endif /* MAIN_H_ */
