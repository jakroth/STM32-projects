#include <stdint.h>

int main(void)
{
	// using the volatile keyword stops the optimizer from optimizing these variables\
	// can prevent overly agressive optimization that breaks the code
	// particularly useful if a variable might be changed or used outside this program
	uint8_t volatile data1;
	uint8_t volatile data2;

	data1 = 50;

	data2 = data1;
	data2 = data1;

	for(;;);
}
